# Where IS Document ROOT directory:
# sudo leafpad /etc/lighttpd/lighttpd.conf
sudo mkdir     /var/log/DranpumpData
sudo chown pi  /var/log/DranpumpData
sudo chmod 777 /var/log/DranpumpData
sudo mkdir     /var/log/lighttpd
sudo chmod 777 /var/log/lighttpd
/etc/init.d/lighttpd start


# /etc/rc.local
mkdir /var/log/DranpumpData
chown pi /var/log/DranpumpData
chmod 740 /var/log/DranpumpData
sudo mkdir /var/log/lighttpd
chmod 740 /var/log/lighttpd
/etc/init.d/lighttpd start
